<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TabPane fx:id="mTabPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="110.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
			<tabs>
				<Tab fx:id="mExcelTab" text="Excel">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
							<children>
								<VBox alignment="CENTER_LEFT" layoutX="148.0" layoutY="37.0" prefHeight="150.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<ComboBox fx:id="mExcelCombobox" prefWidth="150.0" promptText="%combobox.promptText">
											<VBox.margin>
												<Insets />
											</VBox.margin>
										</ComboBox>
										<HBox alignment="CENTER_LEFT" spacing="15.0">
											<children>
												<TextField fx:id="mExcelTextField" prefWidth="400.0" promptText="%textfield.promptText" />
												<Button fx:id="mExcelButton" mnemonicParsing="false" />
											</children>
										</HBox>
									</children>
									<padding>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</padding>
								</VBox>
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab fx:id="mXMLTab" text="XML">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
							<children>
								<HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="150.0" prefWidth="100.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<TextField fx:id="mXMLTextField" prefWidth="400.0" promptText="%textfield.promptText" />
										<Button fx:id="mXMLButton" mnemonicParsing="false" />
									</children>
									<padding>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</padding>
								</HBox>
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
		<HBox alignment="CENTER_RIGHT" prefHeight="30.0" spacing="30.0">
			<children>
				<Button fx:id="mLogsButton" mnemonicParsing="false" text="%logs.button.label" />
				<Button fx:id="mGenerateButton" mnemonicParsing="false" text="%generate.button.label" />
			</children>
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</HBox>
	</children>
</fx:root>
